  <DataSets>
    <DataSet Name="Matrix_DataSet">
      <Query>
        <DataSourceName>Matrix_DataSource</DataSourceName>
        <CommandText>SELECT dc.ContactNo, dc.customer_type, da.AlternateHotelID, da.market_segment, dl.address, dl.postalCode, dl.city, dl.country, fs.ServiceID, fs.meal, fs.stays_in_week_nights, fs.total_of_special_requests
FROM     FactService AS fs INNER JOIN
                  DimCustomer AS dc ON fs.CustomerKey = dc.CustomerSK INNER JOIN
                  DimAccommodation AS da ON fs.AccommodationKey = da.AccommodationSK INNER JOIN
                  DimLocation AS dl ON fs.LocationKey = dl.LocationSK</CommandText>
      </Query>
